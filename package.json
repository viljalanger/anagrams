{
	"name": "anagrams",
	"version": "0.0.0",
	"license": "MIT",
	"description": "",
	"main": "dist/main.js",
	"scripts": {
		"build": "webpack --config webpack.config.js",
		"start": "npm run build && node dist/main.js",
		"start:dev": "npm run nodemon src/main.ts",
		"nodemon": "nodemon --exec 'ts-node --project tsconfig.json -r tsconfig-paths/register'",
		"test": "jest --config ./jest.config.js",
		"coverage": "rimraf ./coverage && jest --config ./jest.config.js --coverage",
		"coverage:web": "npm run coverage && http-server ./coverage/lcov-report -o",
		"eslint": "eslint \"src/**/*.ts\"",
		"eslint:fix": "npm run eslint --fix",
		"prettier": "prettier \"**/*.ts\"",
		"prettir:fix": "npm run prettier --write",
		"precommit": "npm run eslint:fix && npm run pretteir:fix",
		"prepush": "npm run test"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "prepush"
		}
	},
	"lint-staged": {
		"*.{js,json,ts}": [
			"npm run precommit",
			"git add"
		]
	},
	"author": "Yiyi Chen",
	"devDependencies": {
		"@types/jest": "^26.0.19",
		"@types/node": "^14.14.12",
		"@types/prompts": "^2.0.9",
		"@typescript-eslint/eslint-plugin": "^4.9.1",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^7.0.0",
		"eslint": "^7.15.0",
		"eslint-config-prettier": "^7.0.0",
		"eslint-plugin-security": "^1.4.0",
		"eslint-plugin-security-node": "^1.0.14",
		"file-replace-loader": "^1.3.2",
		"http-server": "^0.12.3",
		"husky": "^4.3.5",
		"jest": "^26.6.3",
		"jest-mock-extended": "^1.0.10",
		"lint-staged": "^10.5.3",
		"nodemon": "^2.0.6",
		"prettier": "^2.2.1",
		"rimraf": "^2.7.1",
		"terser-webpack-plugin": "^5.0.3",
		"ts-jest": "^26.4.4",
		"ts-loader": "^8.0.12",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"tsconfig-paths-webpack-plugin": "^3.3.0",
		"tslib": "^2.0.3",
		"typescript": "^4.1.2",
		"webpack": "^5.10.1",
		"webpack-cli": "^4.2.0",
		"webpack-merge": "^5.7.0"
	},
	"dependencies": {
		"inversify": "^5.0.5",
		"prompts": "^2.4.0",
		"reflect-metadata": "^0.1.13",
		"tslog": "^3.0.1"
	}
}
